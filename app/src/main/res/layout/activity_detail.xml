    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/chartStockHistory"
        android:layout_width="match_parent"
        android:layout_height="420dp">

    </com.github.mikephil.charting.charts.LineChart>

</RelativeLayout>
